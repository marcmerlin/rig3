# Sites config file for z_last/test_rig3_live
# all paths are relative to the testdata dir.
# that's an issue for generated content, which should not be in the checked out dir

[serve]
sites = rig3_live, rig3_atom_live

[DEFAULT]
sources     = dir:album, file:album
dest_dir    = live_dest
base_url    =
cat_filter  = !exclude-me
rig_base    = http://rig.base.url/photos/index.php

[rig3_live]
theme = default
public_name = Rig3 Live
tracking_code = <i>tracking code here</i>

[rig3_atom_live]
theme       = atom
public_name = Rig3 Live Feed

