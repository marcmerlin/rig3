# Sites config file for z_last/test_rig3_live
# all paths are relative to the testdata dir.
# that's an issue for generated content, which should not be in the checked out dir
#
# ** All paths are relative to the testdata dir **

[serve]
sites = rig3_live

[DEFAULT]
sources1    = blog: album/blog1, rig_base: http://rig.base.url/photos1/index.php
sources2    = blog: album/blog2, rig_base: http://rig.base.url/photos2/index.php
dest_dir    = live_dest
base_url    =
cat_filter  = !exclude-me

[rig3_live]
theme = default
public_name = Rig3 Live
tracking_code = <i>tracking code here</i>
toc_categories = !foo
reverse_categories = bar
